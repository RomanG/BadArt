<h1>Listing multi_boards</h1>

<table>
  <tr>
    <th>Link</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @multi_boards.each do |multi_board| %>
  <tr>
    <td><%= multi_board.link %></td>
    <td><%= link_to 'Show', multi_board %></td>
    <td><%= link_to 'Edit', edit_multi_board_path(multi_board) %></td>
    <td><%= link_to 'Destroy', multi_board, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Multi board', new_multi_board_path %>
